# modulo cargado en L2P1P2.py

def factorial(n):
    '''
    Retorna el factorial de un numero natural n.
    Ejemplo: factorial(3)=3*2*1=6
    '''
    salida = 1
    for i in xrange(1,n+1):
        salida *= i
    #
    print salida
    return salida

def fibonacci(n):
    '''
    Retorna los n (con n>2) primeros numeros de la secuencia de Fibonacci
    '''
#    n  = 5
    f0 = 1
    f1 = 1
    salida = [f0,f1]
    #
    for i in xrange(n-2):
        aux = f0+f1
        f0  = f1
        f1  = aux
        #
        salida.append(aux)
    #
    return salida
#
